{
	"id": "2",
	"title": "2. Разговор, общие фразы",
	"phrases": [
		{
			"translation": "Как интересно!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.1t.mp3"],
					"text": "kuin interesno!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.1s.mp3"],
					"text": "Voi, kuin mukava!"
				}
			]
		},
		{
			"translation": "Надо же!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.2t.mp3"],
					"text": "nado že!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.2s.mp3"],
					"text": "Niinkö?!"
				}
			]
		},
		{
			"translation": "да",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.3t.mp3"],
					"text": "da"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.3s.mp3"],
					"text": "Kyllä"
				}
			]
		},
		{
			"translation": "Он приехал?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.4t.mp3"],
					"text": "hiän tuli?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.4s.mp3"],
					"text": "Hiänkö tuli?"
				}
			]
		},
		{
			"translation": "Да, приехал / нет",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.5t.mp3"],
					"text": "da, tuli / ei"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.5s.mp3"],
					"text": "Kyllä, hiän še on / Ei Tämä ei ole"
				}
			]
		},
		{
			"translation": "действительно!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.6t.mp3"],
					"text": "toveštah!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.6s.mp3"],
					"text": "Tovellah!"
				}
			]
		},
		{
			"translation": "сделай, пожалуйста!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.7t.mp3"],
					"text": "luaji, ole hyvä!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.7s.mp3"],
					"text": "Luaji še, ole hyvä!"
				}
			]
		},
		{
			"translation": "Как жалко",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.8t.mp3"],
					"text": "kuin žuali on"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.8s.mp3"],
					"text": "Kuin šiäli on!"
				}
			]
		},
		{
			"translation": "Ты по-карельски говоришь?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.9t.mp3"],
					"text": "pagizetgo šie karielakši?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.9s.mp3"],
					"text": "Pakajatko karjalakši?"
				}
			]
		},
		{
			"translation": "Я по-карельски немножко говорю",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.10t.mp3"],
					"text": "mie vähäzen pagizen karielakši"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.10s.mp3"],
					"text": "Mie pakajan vähän karjalakši"
				}
			]
		},
		{
			"translation": "Я по-карельски только понимаю немножко",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.11t.mp3"],
					"text": "mie karielakši vain vähäzen maltan"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.11s.mp3"],
					"text": "Mie karjalakši vain vähän maltan/ymmärrän"
				}
			]
		},
		{
			"translation": "Язык наш забывается",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.12t.mp3"],
					"text": "kieli miän unahtuu"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.12s.mp3"],
					"text": "Meijän kieli unohtuu"
				}
			]
		},
		{
			"translation": "Язык наш надо сохранять",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.13t.mp3"],
					"text": "miän kiel’dä pidäy äbäžöijä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.13s.mp3"],
					"text": "Meijän kieli pitäy šäilyttyä"
				}
			]
		},
		{
			"translation": "С детьми надо по-карельски говорить",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.14t.mp3"],
					"text": "lapšinke pidäy karielakši paissa"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.14s.mp3"],
					"text": "Lapšien kera pitäy paissa omalla kielellä"
				}
			]
		},
		{
			"translation": "Скажи помедленнее",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.15t.mp3"],
					"text": "pagize hillembäzeh"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.15s.mp3"],
					"text": "Pakaja hitahammin"
				}
			]
		},
		{
			"translation": "Ещё раз повтори",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.16t.mp3"],
					"text": "šano vielä kerda"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.16s.mp3"],
					"text": "Šano vielä kerran"
				}
			]
		},
		{
			"translation": "Понятно!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.17t.mp3"],
					"text": "maltan!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.17s.mp3"],
					"text": "Maltan Ymmärrän"
				}
			]
		},
		{
			"translation": "Давайте по-карельски поговорим",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.18t.mp3"],
					"text": "davaigua karielakši pagizomah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.18s.mp3"],
					"text": "Paiskah karjalakši!"
				}
			]
		},
		{
			"translation": "У меня диалект немножко другой",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.19t.mp3"],
					"text": "miun murreh vähäzet toista on"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.19s.mp3"],
					"text": "Miun pakinatapa on vähän toisenmoini"
				}
			]
		},
		{
			"translation": "Как красиво!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.20t.mp3"],
					"text": "kuin šomah on!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.20s.mp3"],
					"text": "Kuin čoma še on!"
				}
			]
		},
		{
			"translation": "Она мастерица",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.21t.mp3"],
					"text": "hiän on nerokaš"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.21s.mp3"],
					"text": "Hiän on nerokaš Hiän on muasteri"
				}
			]
		},
		{
			"translation": "Какой у неё красивый голос",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.22t.mp3"],
					"text": "hänellä on šoma ijäni"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.22s.mp3"],
					"text": "Hänellä on čoma iäni"
				}
			]
		},
		{
			"translation": "Какой красивый платок",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.23t.mp3"],
					"text": "šoma paikka on"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.23s.mp3"],
					"text": "Kuin kaunis paikka hänellä on"
				}
			]
		},
		{
			"translation": "Погодка холодная",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.24t.mp3"],
					"text": "šiä on vilu"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.24s.mp3"],
					"text": "Šiä on vilu tänäpiänä"
				}
			]
		},
		{
			"translation": "какая погода сегодня?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.25t.mp3"],
					"text": "mytyš šiä tänäpiänä on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.25s.mp3"],
					"text": "Mimmoni šiä tänäpiänä on?"
				}
			]
		},
		{
			"translation": "Вот бы погода была хорошая",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.26t.mp3"],
					"text": "oliis’ vain šiä hyvä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.26s.mp3"],
					"text": "Olispa šiä hyvä"
				}
			]
		},
		{
			"translation": "(Сегодня) пурга",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.27t.mp3"],
					"text": "tänäpiänä on pöyry"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.27s.mp3"],
					"text": "Tänäpiänä tuiskuttau"
				}
			]
		},
		{
			"translation": "(Сегодня) снег",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.28t.mp3"],
					"text": "tänäpiänä on lumi"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.28s.mp3"],
					"text": "Tänäpiänä lunta šatau"
				}
			]
		},
		{
			"translation": "(Сегодня) дождь",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.29t.mp3"],
					"text": "tänäpiänä on vihma"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.29s.mp3"],
					"text": "Tänäpiänä vihmuu"
				}
			]
		},
		{
			"translation": "(Сегодня) непогода",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.30t.mp3"],
					"text": "tänäpiänä on paha šiä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.30s.mp3"],
					"text": "Tänäpiänä on paha šiä"
				}
			]
		},
		{
			"translation": "(Сегодня) жарко",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.31t.mp3"],
					"text": "tänäpiänä on ägie"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.31s.mp3"],
					"text": "Tänäpiänä on äkie"
				}
			]
		},
		{
			"translation": "(Сегодня) холодно",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.32t.mp3"],
					"text": "tänäpiänä on vilu"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.32s.mp3"],
					"text": "Tänäpiänä on kylmä"
				}
			]
		},
		{
			"translation": "Комаров (нынче) много",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.33t.mp3"],
					"text": "šiäštä nyt on äijä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.33s.mp3"],
					"text": "Šiäkšie on äijän"
				}
			]
		},
		{
			"translation": "Мошки (нынче) много",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.34t.mp3"],
					"text": "tihtä nyt on äijä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.34s.mp3"],
					"text": "Mäkäryä on äijän"
				}
			]
		},
		{
			"translation": "Комаров нынче мало",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.35t.mp3"],
					"text": "šiäštä nyt on vähä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.35s.mp3"],
					"text": "Šiäkšie on vähän37"
				}
			]
		},
		{
			"translation": "Надоели комары!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.36t.mp3"],
					"text": "nadoimittih šiäkšet!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.36s.mp3"],
					"text": "Šiäkšet ihnaššutetah"
				}
			]
		},
		{
			"translation": "Надоели мошки!",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.37t.mp3"],
					"text": "nadoimittih tihet!"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.37s.mp3"],
					"text": "Mäkärä kylläššyttäy"
				}
			]
		},
		{
			"translation": "Сядь",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.38t.mp3"],
					"text": "istuoče"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.38s.mp3"],
					"text": "Istuuvu!"
				}
			]
		},
		{
			"translation": "Сядь, пожалуйста",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.39t.mp3"],
					"text": "istuoče, ole hyvä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.39s.mp3"],
					"text": "Issu, ole hyvä!"
				}
			]
		},
		{
			"translation": "Встань",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.40t.mp3"],
					"text": "nouže"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.40s.mp3"],
					"text": "Nouše!"
				}
			]
		},
		{
			"translation": "Пропусти меня",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.41t.mp3"],
					"text": "lašše milma"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.41s.mp3"],
					"text": "Laše milma!"
				}
			]
		},
		{
			"translation": "Заходи",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.42t.mp3"],
					"text": "tule"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.42s.mp3"],
					"text": "Tule!"
				}
			]
		},
		{
			"translation": "Иди сюда",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.43t.mp3"],
					"text": "tule tänne"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.43s.mp3"],
					"text": "Tule tänne!"
				}
			]
		},
		{
			"translation": "Давай перекусим",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.44t.mp3"],
					"text": "davai šyöllämmä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.44s.mp3"],
					"text": "Ruvekka šyömäh!"
				}
			]
		},
		{
			"translation": "Давай чаю попьём",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/2/2.45t.mp3"],
					"text": "davai čuajuo juollamma"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/2/2.45s.mp3"],
