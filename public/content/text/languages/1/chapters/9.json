{
	"id": "9",
	"title": "9. Общение в интернете",
	"phrases": [
		{
			"translation": "Посмотри мой новый пост/рилс/строис",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.1t.mp3"],
					"text": "kačo miun uuži posta/video/storis"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.1s.mp3"],
					"text": "Kačo miun uuši julkaisu/video/storis"
				}
			]
		},
		{
			"translation": "Подпишись на меня",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.2t.mp3"],
					"text": "kirjuttuače miun peryštäh (основу)"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.2s.mp3"],
					"text": "Kirjuttauvu miun šivulla"
				}
			]
		},
		{
			"translation": "Добавь меня в друзья",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.3t.mp3"],
					"text": "liziä milma tovarissoih"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.3s.mp3"],
					"text": "Lisyä miut yštävih"
				}
			]
		},
		{
			"translation": "Закажи мне в интернете",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.4t.mp3"],
					"text": "luajita miula internetašša"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.4s.mp3"],
					"text": "Tilua še internetissä"
				}
			]
		},
		{
			"translation": "Где ты это купила",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.5t.mp3"],
					"text": "missä šie tämän oššiit?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.5s.mp3"],
					"text": "Missä šie oššit tämän"
				}
			]
		},
		{
			"translation": "Я заказала на озоне",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.6t.mp3"],
					"text": "mie luajitiin ozonašša"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.6s.mp3"],
					"text": "Mie tilasin ozonissa"
				}
			]
		},
		{
			"translation": "Скинь деньги на карту",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.7t.mp3"],
					"text": "lähetä / viškua miula rahat/dengat kartalla"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.7s.mp3"],
					"text": "Lähetä miula rahua kortilla"
				}
			]
		},
		{
			"translation": "Перешли мне его сообщение",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.8t.mp3"],
					"text": "työnnä miula hänen viestin"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.8s.mp3"],
					"text": "Lähetä miula hänen viesti"
				}
			]
		},
		{
			"translation": "Добавь в группу/диалог",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.9t.mp3"],
					"text": "kuču partih / paginah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.9s.mp3"],
					"text": "Kuču miut dialogih/pakinah"
				}
			]
		},
		{
			"translation": "Давай создадим чат",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.10t.mp3"],
					"text": "davai luajimma paginan"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.10s.mp3"],
					"text": "Luajikkah yhtehini diaigi/pakina"
				}
			]
		},
		{
			"translation": "В группе давно публикаций не было",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.11t.mp3"],
					"text": "gruupašša/partissa jo ammuin eulun postie"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.11s.mp3"],
					"text": "Ryhmäššä ei ollun julkaisuja jo ammuin"
				}
			]
		},
		{
			"translation": "Он поставил лайк",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.12t.mp3"],
					"text": "hiän klikkai armahukšen"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.12s.mp3"],
					"text": "Hiän pani tykkäykšen"
				}
			]
		},
		{
			"translation": "Я лайкнул",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.13t.mp3"],
					"text": "mie klikkain armahukšen"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.13s.mp3"],
					"text": "Mie tykkäsin"
				}
			]
		},
		{
			"translation": "Я оставил комментарий",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.14t.mp3"],
					"text": "mie jätiin kommentarijän"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.14s.mp3"],
					"text": "Mie jätin kommentin"
				}
			]
		},
		{
			"translation": "Сделай репост",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.15t.mp3"],
					"text": "luaji reposta / kaimua (отправить) kirjutukšen"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.15s.mp3"],
					"text": "Pane omalla šivulla"
				}
			]
		},
		{
			"translation": "Как написать š/č ä,ö",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/9/9.16t.mp3"],
					"text": "kuin kirjuttua bukvie š/č ä,ö?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/9/9.16s.mp3"],
					"text": "Kuin kirjuttua internetissä š/č ä,ö kirjaimie?"
				}
			]
		}
	]
}
