{
	"id": "4",
	"title": "4. Вопросы",
	"phrases": [
		{
			"id": 0,
			"translation": "Вопросы",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Kyžymykšet" },
				{ "id": "2", "title": "Северный", "text": "Kyšymykšet" }
			]
		},
		{
			"id": 1,
			"translation": "Кто ты?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Ken šie olet?" },
				{ "id": "2", "title": "Северный", "text": "Ken šie olet?" }
			]
		},
		{
			"id": 2,
			"translation": "Кто пойдет в музей?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Ken lähtöy muzeih?" },
				{ "id": "2", "title": "Северный", "text": "Ken lähtöy musejoh?" }
			]
		},
		{
			"id": 3,
			"translation": "У кого есть ручка, карандаш?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kellä on ručka, krandaša?" },
				{ "id": "2", "title": "Северный", "text": "Kellä on kynä?" }
			]
		},
		{
			"id": 4,
			"translation": "Кому отдать книгy?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kellä andua kirjan?" },
				{ "id": "2", "title": "Северный", "text": "Kellä antua kirjan?" }
			]
		},
		{
			"id": 5,
			"translation": "С кем ты придешь?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kenenke šie tulet?" },
				{ "id": "2", "title": "Северный", "text": "Kenen kera šie tulet?" }
			]
		},
		{
			"id": 6,
			"translation": "Что это?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Mi tämä on?" },
				{ "id": "2", "title": "Северный", "text": "Mi tämä on?" }
			]
		},
		{
			"id": 7,
			"translation": "Что ты сказал? ",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Midä šie šanoit?" },
				{ "id": "2", "title": "Северный", "text": "Mitä šie šanoit?" }
			]
		},
		{
			"id": 8,
			"translation": "О чем ты расскажешь?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Midä šie šanelet?" },
				{ "id": "2", "title": "Северный", "text": "Mistä šie kerrot?" }
			]
		},
		{
			"id": 9,
			"translation": "Чья это сумка?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Kenen tämä sumča on?" },
				{ "id": "2", "title": "Северный", "text": "Kenen tämä laukku on?" }
			]
		},
		{
			"id": 10,
			"translation": "Который автобус наш?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Kumbane avtobussa on miän?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Kumpani autobušši on meijän?"
				}
			]
		},
		{
			"id": 11,
			"translation": "Который (из двух)? из вас Антти?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Ken teistä on Antti?" },
				{ "id": "2", "title": "Северный", "text": "Kumpi teistä on Antti?" }
			]
		},
		{
			"id": 12,
			"translation": "Которая (по счету) это остановка?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Monešgo piettyš tämä on?" },
				{ "id": "2", "title": "Северный", "text": "Moneško pyšäkki še on?" }
			]
		},
		{
			"id": 13,
			"translation": "Сколько стоит? ",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "äijägo tämä makšau?" },
				{
					"id": "2",
					"title": "Северный",
					"text": "Äijänkö še makšau? Mitä še makšau?"
				}
			]
		},
		{
			"id": 14,
			"translation": "Сколько домов в деревне?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "äijägo taluo on kyläššä?" },
				{ "id": "2", "title": "Северный", "text": "Montako taluo kyläššä on?" }
			]
		},
		{
			"id": 15,
			"translation": "Когда поезд уходит?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Konža pojezda lähtöy?" },
				{ "id": "2", "title": "Северный", "text": "Konša juna lähtöy?" }
			]
		},
		{
			"id": 16,
			"translation": "Как долго  ты будешь в Петрозаводске?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "min viikkuo šie lienet Petroskoissa?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Kuin pitän aikua šie olet Petroskoissa?"
				}
			]
		},
		{
			"id": 17,
			"translation": "Где находится магазин?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "missä on laukka?" },
				{ "id": "2", "title": "Северный", "text": "Missä on kauppa?" }
			]
		},
		{
			"id": 18,
			"translation": "Kуда эта дорога ведет? ",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kunne mänöy tämä tie?" },
				{ "id": "2", "title": "Северный", "text": "Kunne/minne tämä tie viey?" }
			]
		},
		{
			"id": 19,
			"translation": "Откуда дует?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mistä tuulou?" },
				{ "id": "2", "title": "Северный", "text": "Mistä tuulou?" }
			]
		},
		{
			"id": 20,
			"translation": "Как  нам проехать на автовокзал?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Kuin meilä ajua avtovokzualalla?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Mitein myö piäšemmä autobuššiašemalla?"
				}
			]
		},
		{
			"id": 21,
			"translation": "Почему ты смеешься?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Mintän šie nagrat?" },
				{ "id": "2", "title": "Северный", "text": "Mintäh šie nakrat?" }
			]
		},
		{
			"id": 22,
			"translation": "Во сколько ты встаешь?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "konža šie noužet?" },
				{ "id": "2", "title": "Северный", "text": "Moneltako šie noušet?" }
			]
		},
		{
			"id": 23,
			"translation": "На чем я доеду до вокзала?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Millä miula ajua vokzualah?"
				},
				{ "id": "2", "title": "Северный", "text": "Millä mie piäšen ašemalla?" }
			]
		},
		{
			"id": 24,
			"translation": "Какое сегодня число?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Monešgo päivä tänäpiänä on?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Moneško päivä tänäpiänä on?"
				}
			]
		},
		{
			"id": 25,
			"translation": "Что хочешь?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "midä tahot?" },
				{ "id": "2", "title": "Северный", "text": "Mitä tahot?" }
			]
		},
		{
			"id": 26,
			"translation": "Который час?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "äijägo aigua on?" },
				{ "id": "2", "title": "Северный", "text": "Äijänkö kello on?" }
			]
		},
		{
			"id": 27,
			"translation": "Смогу ли я дойти до театра пешком?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "voičengo mie kopittua teatrah šuat jalgaziin?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Piäšenkö mie teatterih aštumalla?"
				}
			]
		},
		{
			"id": 28,
			"translation": "Сколько времени  мне надо ждать?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "äijägo aigua miula pidäy vuottua?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Pitän aikuako/pitältikö miun pitäy vuottua?"
				}
			]
		}
	]
}
