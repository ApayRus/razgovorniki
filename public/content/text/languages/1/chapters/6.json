{
	"id": "6",
	"title": "6. Приготовление пищи, трапеза",
	"phrases": [
		{
			"id": 0,
			"translation": "Приготовление пищи. Трапеза",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Emannuš.  šyöndä" },
				{ "id": "2", "title": "Северный", "text": "Ruoan laittamini. Šyömini" }
			]
		},
		{
			"id": 1,
			"translation": "Что сегодня будем готовить?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "midä tänäpiänä rubiemma varuštamah?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Mitä rupiemma keittämäh tänäpiänä?"
				}
			]
		},
		{
			"id": 2,
			"translation": "Сегодня суп сварим.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "tänäpiänä keitämmä rokkua" },
				{
					"id": "2",
					"title": "Северный",
					"text": "Tänäpiänä keitämmä keittuo."
				}
			]
		},
		{
			"id": 3,
			"translation": "Рыбу сварим / рыбу пожарим.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "kalua keitämmä / kalua žuarimma"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Keitämmä kalua/suarimma kalua"
				}
			]
		},
		{
			"id": 4,
			"translation": "Мясо сварим.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "keitämmä lihua" },
				{ "id": "2", "title": "Северный", "text": "Keitämmä lihua." }
			]
		},
		{
			"id": 5,
			"translation": "Ты поставил кастрюлю на печку?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "šie paniit kostr’ul’an kiugualla?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Panitko šie kattilan kiukualla?"
				}
			]
		},
		{
			"id": 6,
			"translation": "Воду налил в кастрюлю? - Налил.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "vettä paniit kostr’ul’ah? – paniin."
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Kuavoitko vettä kattilah? Kuavoin."
				}
			]
		},
		{
			"id": 7,
			"translation": "Я картошку почистил.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mie kuoritiin muajuablokkua"
				},
				{ "id": "2", "title": "Северный", "text": "Mie kuorin potakkua." }
			]
		},
		{
			"id": 8,
			"translation": "Рыбу нарезала.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "leikkain palaziksi kalan" },
				{ "id": "2", "title": "Северный", "text": "Mie leikkasin kalua. " }
			]
		},
		{
			"id": 9,
			"translation": "Посоли (воду в кастрюле).",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "šuolua (vettä kostr’ul’ašša)"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Pane šuolua veteh / Šuolua vettä kattilašša"
				}
			]
		},
		{
			"id": 10,
			"translation": "Тесто поставь (намешай).",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "šeizota tahaš" },
				{ "id": "2", "title": "Северный", "text": "Pane taikinua" }
			]
		},
		{
			"id": 11,
			"translation": "Немножко соли не хватает",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "vähäzen šuolua ei fatai" },
				{
					"id": "2",
					"title": "Северный",
					"text": "Vielä vähäsen šuolua ei riitä"
				}
			]
		},
		{
			"id": 12,
			"translation": "Многовато соли положила",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "äijäzen šuolua paniit" },
				{ "id": "2", "title": "Северный", "text": "Liian äijyä šuolua panit" }
			]
		},
		{
			"id": 13,
			"translation": "Поставь хлеб в печку!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "pane leibä kiuguah!" },
				{ "id": "2", "title": "Северный", "text": "Pane leipyä kiukuah" }
			]
		},
		{
			"id": 14,
			"translation": "Ждем когда испечется.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "vuotamma konža paistuu." },
				{
					"id": "2",
					"title": "Северный",
					"text": "Vuotamma, konša leipä on valmis"
				}
			]
		},
		{
			"id": 15,
			"translation": "Хлеб испечется, с горячим хлебом суп будем кушать.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "leibä paistuu, palavanke leivänke rubiemma šyömäh rokkua."
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Leipä on valmis, rupiemma šyömäh šitä kuuman keiton kera"
				}
			]
		},
		{
			"id": 16,
			"translation": "В следующий раз суп сварим.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Toizeh kerdah keitämmä rokan."
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Enši kertua keitämmä rokkua"
				}
			]
		},
		{
			"id": 17,
			"translation": "Мясо опустим в кастрюлю, пусть поварится.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "panemma liha kostr’ul’ah, ana keittyy."
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Panemma lihua kattilah, anna keittyy"
				}
			]
		},
		{
			"id": 18,
			"translation": "Немножко пусть поварится – потом опускаем картошку.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "vähäzen ana keittyy, šidä panemma muajuablokan"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Anna še vähäsen keittyy, šiitä panemma potakkua"
				}
			]
		},
		{
			"id": 19,
			"translation": "Потом лапшу и лук опускаем.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "šidä panemma makaronua da luukkua."
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Šiitä panemma makaronie ta luukkuo"
				}
			]
		},
		{
			"id": 20,
			"translation": "Еще немного поварится, и готово!",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "vielä vähäzen keityy da i valmis’!"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Anna še vielä vähäsen kiehuu ta on valmis"
				}
			]
		},
		{
			"id": 21,
			"translation": "Дети, идите кушать!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "lapšet, tulgua šyömäh!" },
				{ "id": "2", "title": "Северный", "text": "Lapšet, tulkua šyömäh" }
			]
		},
		{
			"id": 22,
			"translation": "Спасибо!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "passibo!" },
				{ "id": "2", "title": "Северный", "text": "Passipo!" }
			]
		},
		{
			"id": 23,
			"translation": "Суп вкусный!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "magie rokka!" },
				{ "id": "2", "title": "Северный", "text": "Kuin rokka on makie!" }
			]
		},
		{
			"id": 24,
			"translation": "Как хорошо! Спасибо!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kuin hyviin! Passibo!" },
				{ "id": "2", "title": "Северный", "text": "Kuin hyvin on, passipo!" }
			]
		},
		{
			"id": 25,
			"translation": "Как я наелась/ся!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Kuin mie kylläššyin!" },
				{ "id": "2", "title": "Северный", "text": "Kuin mie kylläššyin!" }
			]
		},
		{
			"id": 26,
			"translation": "Ой как вкусно было!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kuin oli magie!" },
				{ "id": "2", "title": "Северный", "text": "Voi, kuin makie oli!" }
			]
		},
		{
			"id": 27,
			"translation": "Мне еще супу налей!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Pane miula vielä rokkua!" },
				{ "id": "2", "title": "Северный", "text": "Pane miula vielä rokkua" }
			]
		},
		{
			"id": 28,
			"translation": "иди руки помой!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mäne peže kiät!" },
				{ "id": "2", "title": "Северный", "text": "Mäne ta peše kiät!" }
			]
		},
		{
			"id": 29,
			"translation": "Иди садись!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mäne istuoče!" },
				{ "id": "2", "title": "Северный", "text": "Mäne ta issu!" }
			]
		},
		{
			"id": 30,
			"translation": "Иди помоги стол накрыть.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mäne auta stolalla šyömizet panna!"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Tule auttamah stolua kattuamah"
				}
			]
		},
		{
			"id": 31,
			"translation": "Кушай!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "šyö!" },
				{ "id": "2", "title": "Северный", "text": "Šyö" }
			]
		},
		{
			"id": 32,
			"translation": "Суп горячий!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "rokka palava" },
				{ "id": "2", "title": "Северный", "text": "Rokka on palava" }
			]
		},
		{
			"id": 33,
			"translation": "Соль принеси.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "tuo šuolua" },
				{ "id": "2", "title": "Северный", "text": "Tuo šuolua" }
			]
		},
		{
			"id": 34,
			"translation": "Принеси / подай соль.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "tuo/annalla šuolua" },
				{ "id": "2", "title": "Северный", "text": "Tua/anna šuolua" }
			]
		},
		{
			"id": 35,
			"translation": "Сахар подай.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "annalla suaharuo" },
				{ "id": "2", "title": "Северный", "text": "Anna šokerie" }
			]
		},
		{
			"id": 36,
			"translation": "Подуй немного (детям)",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "puhu vähäzen (lapšilla)" },
				{ "id": "2", "title": "Северный", "text": "Puhu vähäsen." }
			]
		},
		{
			"id": 37,
			"translation": "Немного подожди, пусть остынет.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "vuota vähäzen, ana jähtyy" },
				{ "id": "2", "title": "Северный", "text": "Vuota vähäsen, anna jähtyy" }
			]
		},
		{
			"id": 38,
			"translation": "Сиди спокойно! (если ребенок балуется)",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "issu tyynylleh!" },
				{ "id": "2", "title": "Северный", "text": "Issu rauhašša" }
			]
		},
		{
			"id": 39,
			"translation": "Кушай, не разговаривай!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "šyö, elä pagize" },
				{ "id": "2", "title": "Северный", "text": "šyö ta elä pakaja" }
			]
		},
		{
			"id": 40,
			"translation": "Не торопись!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "elä kiirähä" },
				{ "id": "2", "title": "Северный", "text": "elä kiirehä" }
			]
		},
		{
			"id": 41,
			"translation": "Я еще есть хочу (не наелся).",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mie vielä tahon šyvvä (en kylläššyn)"
				},
				{ "id": "2", "title": "Северный", "text": "Mie tahon vielä šyyvvä" }
			]
		},
		{
			"id": 42,
			"translation": "Немного не хватило (не наелся).",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "vähäzen ei fatti (en kylläššyn)"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Vielä vähäsen himottais šyyvvä"
				}
			]
		},
		{
			"id": 43,
			"translation": "Я хочу горячего чаю!",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mie tahon palavua čuajuo!" },
				{ "id": "2", "title": "Северный", "text": "Tahon kuumua čäijyö" }
			]
		},
		{
			"id": 44,
			"translation": "Я чай не буду пить.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mie čuajuo en rubie juomah"
				},
				{ "id": "2", "title": "Северный", "text": "Mie en rupie čäijyö juomah" }
			]
		},
		{
			"id": 45,
			"translation": "Холодную воду попью.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "juon viluo vettä" },
				{ "id": "2", "title": "Северный", "text": "Juon viluo vettä" }
			]
		},
		{
			"id": 46,
			"translation": "Есть конфеты (к чаю)?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "ollougo kanafetkua (čuajuh)?"
				},
				{ "id": "2", "title": "Северный", "text": "Onko mitänih makieta?" }
			]
		},
		{
			"id": 47,
			"translation": "я с сахаром чай не пью.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mie suaharonke en juo." },
				{ "id": "2", "title": "Северный", "text": "Mie en juo šokerin kera" }
			]
		},
		{
			"id": 48,
			"translation": "Морс сварим.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "keitämmä karbalovettä" },
				{ "id": "2", "title": "Северный", "text": "Keitämmä morššie" }
			]
		},
		{
			"id": 49,
			"translation": "Сварим морс из брусники.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "keitämmä buolavettä" },
				{ "id": "2", "title": "Северный", "text": "Keitämmä marjavettä" }
			]
		},
		{
			"id": 50,
			"translation": "брусника",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "buola" },
				{ "id": "2", "title": "Северный", "text": "puolukka" }
			]
		},
		{
			"id": 51,
			"translation": "клюква",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "karbalo" },
				{ "id": "2", "title": "Северный", "text": "karpalo" }
			]
		},
		{
			"id": 52,
			"translation": "малина",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "vavarno" },
				{ "id": "2", "title": "Северный", "text": "malina" }
			]
		},
		{
			"id": 53,
			"translation": "черника",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mussikka" },
				{ "id": "2", "title": "Северный", "text": "mussikka" }
			]
		},
		{
			"id": 54,
			"translation": "пирог с ягодами",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "marjapiirua" },
				{ "id": "2", "title": "Северный", "text": "marjapiirua" }
			]
		},
		{
			"id": 55,
			"translation": "пирог с рыбой",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kalapiirua" },
				{ "id": "2", "title": "Северный", "text": "kalapiirua" }
			]
		},
		{
			"id": 56,
			"translation": "пирог с мясом",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "lihapiirua" },
				{ "id": "2", "title": "Северный", "text": "lihapiirua" }
			]
		},
		{
			"id": 57,
			"translation": "пирожки (жареные)",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "obarnikat" },
				{ "id": "2", "title": "Северный", "text": "paissetut piiruat" }
			]
		}
	]
}
