{
	"id": "10",
	"title": "10. В автомобиле",
	"phrases": [
		{
			"id": 0,
			"translation": "В автомобиле",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mašinašša" },
				{ "id": "2", "title": "Северный", "text": " Autošša" }
			]
		},
		{
			"id": 1,
			"translation": "ты заедешь за мной?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "tuletgo/ ajellatgo milma vaš?"
				},
				{ "id": "2", "title": "Северный", "text": "Tuletko milma hakemah?" }
			]
		},
		{
			"id": 2,
			"translation": "я заеду за тобой",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mie tulen/ ajellan šilma vaš"
				},
				{ "id": "2", "title": "Северный", "text": "Mie tulen šiula" }
			]
		},
		{
			"id": 3,
			"translation": "Поедем на моей машине",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "lähemmä ajamah miunomalla mašinalla"
				},
				{ "id": "2", "title": "Северный", "text": "Ajamma miun autolla" }
			]
		},
		{
			"id": 4,
			"translation": "Поедем на твоей машине",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "lähemmä ajamah šiunomalla mašinalla"
				},
				{ "id": "2", "title": "Северный", "text": "Ajamma šiun autolla" }
			]
		},
		{
			"id": 5,
			"translation": "Тебе удобно?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "ongo kaikki hyviin? Ongo šiula skodno?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Onko kaikki hyvin? Onko šiula mukava?"
				}
			]
		},
		{
			"id": 6,
			"translation": "Все хорошо.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kaikki on hyviin" },
				{ "id": "2", "title": "Северный", "text": "Kaikki on hyvin" }
			]
		},
		{
			"id": 7,
			"translation": "Включить музыку?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "panna muuzikkua?" },
				{ "id": "2", "title": "Северный", "text": "Panenko musiikkie piällä?" }
			]
		},
		{
			"id": 8,
			"translation": "Да, включи.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "da, pane" },
				{ "id": "2", "title": "Северный", "text": "Pane vain" }
			]
		},
		{
			"id": 9,
			"translation": "Кто это исполняет?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "ken tädä lauluo laulau?" },
				{ "id": "2", "title": "Северный", "text": "Ken tätä lauluo laulau?" }
			]
		},
		{
			"id": 10,
			"translation": "Это группа Sattuma",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "že on Sattuma-gruuppa" },
				{ "id": "2", "title": "Северный", "text": "Še on Šattuma-ryhmä" }
			]
		},
		{
			"id": 11,
			"translation": "Это поет ...",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "Tämä laulau …" },
				{ "id": "2", "title": "Северный", "text": "Šitä laulau ..." }
			]
		},
		{
			"id": 12,
			"translation": "Давай остановимся.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "davai šeizauvumma" },
				{
					"id": "2",
					"title": "Северный",
					"text": "Voimmako šeisautuo? Konša šeisauvumma?"
				}
			]
		},
		{
			"id": 13,
			"translation": "Я хочу в туалет.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mie tahon ubornoih" },
				{ "id": "2", "title": "Северный", "text": "Mie tahon tualettih." }
			]
		},
		{
			"id": 14,
			"translation": "Я хочу поесть.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mie tahon šyyvä" },
				{ "id": "2", "title": "Северный", "text": "Pitäis šyyvvä" }
			]
		},
		{
			"id": 15,
			"translation": "Я хочу покурить.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "mie tahon kurie" },
				{ "id": "2", "title": "Северный", "text": "Himottais polttua tupakkua" }
			]
		},
		{
			"id": 16,
			"translation": "Куда эта дорога ведет?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "kunne mänöy tämä tie?" },
				{ "id": "2", "title": "Северный", "text": "Kunne tämä tie on?" }
			]
		},
		{
			"id": 17,
			"translation": "Что там за озеро справа/слева виднеется?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "Mi järvi šielä oigiella / važamella pain nägyy?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Mie järvi šieltä oikielta/vašemelta näkyy?"
				}
			]
		},
		{
			"id": 18,
			"translation": "Сколько нам еще ехать?",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "äijägo meilä vielä ajua?" },
				{
					"id": "2",
					"title": "Северный",
					"text": "Kuin äijän meilä vielä ajua?"
				}
			]
		},
		{
			"id": 19,
			"translation": "Еще 2 часа 40 минут.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "vielä kakši čuasuo da nelläkymmendä minuuttua"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Vielä kakši tuntie  nelläkymmentä minuuttie"
				}
			]
		},
		{
			"id": 20,
			"translation": "Не против, если я посплю?",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "etgo šie vaštua, kuin mie maguallan?"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Etkö ole vaštah još mie makuan vähäsen?"
				}
			]
		},
		{
			"id": 21,
			"translation": "Налей мне пожалуйста чай.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "ole hyvä, pane miula čuajuo"
				},
				{ "id": "2", "title": "Северный", "text": "Anna/Kua miula čäijyö" }
			]
		},
		{
			"id": 22,
			"translation": "Дай воды.",
			"dialects": [
				{ "id": "1", "title": "Тверской", "text": "ana vettä" },
				{ "id": "2", "title": "Северный", "text": "Anna vettä" }
			]
		},
		{
			"id": 23,
			"translation": "Какая хорошая/красивая/плохая дорога.",
			"dialects": [
				{
					"id": "1",
					"title": "Тверской",
					"text": "mittynane on hyvä / šoma / paha tie"
				},
				{
					"id": "2",
					"title": "Северный",
					"text": "Kuin hyvä/kaunis/paha tie on"
				}
			]
		}
	]
}
