{
	"id": "10",
	"title": "10. В пути",
	"phrases": [
		{
			"translation": "В автомобиле",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.1t.mp3"],
					"text": "mašinašša"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.1s.mp3"],
					"text": " Autošša"
				}
			]
		},
		{
			"translation": "ты заедешь за мной?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.2t.mp3"],
					"text": "tuletgo/ ajellatgo milma vaš?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.2s.mp3"],
					"text": "Tuletko milma hakemah?"
				}
			]
		},
		{
			"translation": "я заеду за тобой",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.3t.mp3"],
					"text": "mie tulen/ ajellan šilma vaš"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.3s.mp3"],
					"text": "Mie tulen šiula"
				}
			]
		},
		{
			"translation": "Поедем на моей машине",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.4t.mp3"],
					"text": "lähemmä ajamah miunomalla mašinalla"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.4s.mp3"],
					"text": "Ajamma miun autolla"
				}
			]
		},
		{
			"translation": "Поедем на твоей машине",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.5t.mp3"],
					"text": "lähemmä ajamah šiunomalla mašinalla"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.5s.mp3"],
					"text": "Ajamma šiun autolla"
				}
			]
		},
		{
			"translation": "Тебе удобно?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.6t.mp3"],
					"text": "ongo kaikki hyviin? Ongo šiula skodno?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.6s.mp3"],
					"text": "Onko kaikki hyvin? Onko šiula mukava?"
				}
			]
		},
		{
			"translation": "Все хорошо.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.7t.mp3"],
					"text": "kaikki on hyviin"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.7s.mp3"],
					"text": "Kaikki on hyvin"
				}
			]
		},
		{
			"translation": "Включить музыку?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.8t.mp3"],
					"text": "panna muuzikkua?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.8s.mp3"],
					"text": "Panenko musiikkie piällä?"
				}
			]
		},
		{
			"translation": "Да, включи.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.9t.mp3"],
					"text": "da, pane"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.9s.mp3"],
					"text": "Pane vain"
				}
			]
		},
		{
			"translation": "Кто это исполняет?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.10t.mp3"],
					"text": "ken tädä lauluo laulau?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.10s.mp3"],
					"text": "Ken tätä lauluo laulau?"
				}
			]
		},
		{
			"translation": "Это группа Sattuma",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.11t.mp3"],
					"text": "že on Sattuma-gruuppa"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.11s.mp3"],
					"text": "Še on Šattuma-ryhmä"
				}
			]
		},
		{
			"translation": "Это поет ...",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.12t.mp3"],
					"text": "Tämä laulau …"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.12s.mp3"],
					"text": "Šitä laulau ..."
				}
			]
		},
		{
			"translation": "Давай остановимся.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.13t.mp3"],
					"text": "davai šeizauvumma"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.13s.mp3"],
					"text": "Voimmako šeisautuo? Konša šeisauvumma?"
				}
			]
		},
		{
			"translation": "Я хочу в туалет.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.14t.mp3"],
					"text": "mie tahon ubornoih"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.14s.mp3"],
					"text": "Mie tahon tualettih."
				}
			]
		},
		{
			"translation": "Я хочу поесть.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.15t.mp3"],
					"text": "mie tahon šyyvä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.15s.mp3"],
					"text": "Pitäis šyyvvä"
				}
			]
		},
		{
			"translation": "Я хочу покурить.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.16t.mp3"],
					"text": "mie tahon kurie"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.16s.mp3"],
					"text": "Himottais polttua tupakkua"
				}
			]
		},
		{
			"translation": "Куда эта дорога ведет?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.17t.mp3"],
					"text": "kunne mänöy tämä tie?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.17s.mp3"],
					"text": "Kunne tämä tie on?"
				}
			]
		},
		{
			"translation": "Что там за озеро справа/слева виднеется?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.18t.mp3"],
					"text": "Mi järvi šielä oigiella / važamella pain nägyy?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.18s.mp3"],
					"text": "Mie järvi šieltä oikielta/vašemelta näkyy?"
				}
			]
		},
		{
			"translation": "Сколько нам еще ехать?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.19t.mp3"],
					"text": "äijägo meilä vielä ajua?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.19s.mp3"],
					"text": "Kuin äijän meilä vielä ajua?"
				}
			]
		},
		{
			"translation": "Еще 2 часа 40 минут.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.20t.mp3"],
					"text": "vielä kakši čuasuo da nelläkymmendä minuuttua"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.20s.mp3"],
					"text": "Vielä kakši tuntie  nelläkymmentä minuuttie"
				}
			]
		},
		{
			"translation": "Не против, если я посплю?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.21t.mp3"],
					"text": "etgo šie vaštua, kuin mie maguallan?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.21s.mp3"],
					"text": "Etkö ole vaštah još mie makuan vähäsen?"
				}
			]
		},
		{
			"translation": "Налей мне пожалуйста чай.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.22t.mp3"],
					"text": "ole hyvä, pane miula čuajuo"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.22s.mp3"],
					"text": "Anna/Kua miula čäijyö"
				}
			]
		},
		{
			"translation": "Дай воды.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.23t.mp3"],
					"text": "ana vettä"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.23s.mp3"],
					"text": "Anna vettä"
				}
			]
		},
		{
			"translation": "Какая хорошая/красивая/плохая дорога.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/10/10.24t.mp3"],
					"text": "mittynane on hyvä / šoma / paha tie"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/10/10.24s.mp3"],
					"text": "Kuin hyvä/kaunis/paha tie on"
				}
			]
		}
	]
}
