{
	"id": "2",
	"title": "2. Приветствие. Прощание",
	"phrases": [
		{
			"id": 1,
			"translation": "Здравствуйте (одному человеку)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "шумбрат",
					"transcription": "šumbrat (одному)"
				}
			]
		},
		{
			"id": 2,
			"translation": "Здравствуйте (многим людям)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "шумбратада",
					"transcription": "šumbratadə"
				}
			]
		},
		{
			"id": 3,
			"translation": "До завтра",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "вандыс",
					"transcription": "vandis"
				}
			]
		},
		{
			"id": 4,
			"translation": "До завтра ещё дожить надо!",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "вандыс нинга эряма!",
					"transcription": "vandis n'ingə er'ɛma "
				}
			]
		},
		{
			"id": 5,
			"translation": "До свидания",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "няемозонк",
					"transcription": "n'ɛjəməzənk"
				}
			]
		},
		{
			"id": 6,
			"translation": "Ещё приезжайте",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "дага сашендода",
					"transcription": "dagə sašəndəda"
				}
			]
		},
		{
			"id": 7,
			"translation": "Спи хорошо",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "лацкас удофт",
					"transcription": "lackas udəft"
				}
			]
		},
		{
			"id": 8,
			"translation": "Дай Бог здоровья",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "шумбрашине максоза",
					"transcription": "šumbrašin'ə maksəzə"
				}
			]
		},
		{
			"id": 9,
			"translation": "Хорошо вам добраться",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "лацкя пачкодеда",
					"transcription": "lackɛ pačkəd'ədə"
				}
			]
		},
		{
			"id": 10,
			"translation": "Как ваши дела?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "кода тефне?",
					"transcription": "koda t'efn'ə?"
				}
			]
		},
		{
			"id": 11,
			"translation": "Идут",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "молихть",
					"transcription": "mol'ij̊t'"
				}
			]
		},
		{
			"id": 12,
			"translation": "Хорошо!",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "лац",
					"transcription": "lac"
				}
			]
		},
		{
			"id": 13,
			"translation": "Туда-сюда",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сей-тов",
					"transcription": "sej-tov"
				}
			]
		},
		{
			"id": 14,
			"translation": "Потихонечку",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "салаване",
					"transcription": "salavan'ə"
				}
			]
		},
		{
			"id": 15,
			"translation": "Не очень-то",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "аф пяк молихть",
					"transcription": "af pɛk mol'ij̊t'"
				}
			]
		},
		{
			"id": 16,
			"translation": "Как здоровье?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "кода шумбрашице?",
					"transcription": "koda šumbrašic'e?"
				}
			]
		},
		{
			"id": 17,
			"translation": "Болею",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сярядян",
					"transcription": "s'ɛr'ɛd'an"
				}
			]
		},
		{
			"id": 18,
			"translation": "Вы откуда?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "Тинь коста саде?",
					"transcription": "t'in' kostə sad'ə?"
				}
			]
		},
		{
			"id": 19,
			"translation": "какие новости?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "кодамот кулятне?",
					"transcription": "kodamət kul'ɛt'n'ə?"
				}
			]
		},
		{
			"id": 20,
			"translation": "у вас красивый кот",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "тинь катонте кодама мазы",
					"transcription": "tin' katən't'ə kodamə mazit"
				}
			]
		},
		{
			"id": 21,
			"translation": "С Богом!",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "Шкайнять мархта",
					"transcription": "škajn'ɛt' mar̥tə"
				}
			]
		},
		{
			"id": 22,
			"translation": "матери привет передавай",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "тядяцты кулязень азк",
					"transcription": "t'ɛd'ɛncti kul'ɛz'ən' azk"
				}
			]
		}
	]
}
