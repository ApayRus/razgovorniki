{
	"id": "1",
	"title": "1. Язык",
	"phrases": [
		{
			"id": 1,
			"translation": "Как это по-мордовски сказать?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сянь кода мокшекс азомс?",
					"transcription": "s'ɛn' koda mokšəks azəms?"
				}
			]
		},
		{
			"id": 2,
			"translation": "Давайте по-мордовски поговорим",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мокшекс корхнетама!",
					"transcription": "mokšəks kor̥n'ətamə"
				}
			]
		},
		{
			"id": 3,
			"translation": "А как это по-русски?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "а кода тя рузкс?",
					"transcription": "a koda t'ɛ ruzks?"
				}
			]
		},
		{
			"id": 4,
			"translation": "Этого слова я не знаю",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "ва тя валть мон аф содаса",
					"transcription": "va t'ɛ valt' mon af sodasa"
				}
			]
		},
		{
			"id": 5,
			"translation": "Я по-мордовски не говорю",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мон мокшекс аф корхтан",
					"transcription": "mon mokšəks af kor̥tan"
				}
			]
		},
		{
			"id": 6,
			"translation": "Я по-мордовски только немного говорю",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мон мокшекс аф пяк корхтан",
					"transcription": "mon mokšəks af pɛk kor̥tan"
				}
			]
		},
		{
			"id": 7,
			"translation": "Я по-мордовски понимаю, а не говорю",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мон мокшекс аньцек шарьходян, аф корхтан",
					"transcription": "mon mokšəks anc'ək šar̥kəd'an, af kor̥tan"
				}
			]
		},
		{
			"id": 8,
			"translation": "Он по-мордовски всё понимает, но не говорит",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сон сембонь шарьхкоди, но аф корхтай",
					"transcription": "son s'embən' šar̥kəd'i, no af kor̥taj"
				}
			]
		},
		{
			"id": 9,
			"translation": "Повторите ещё раз",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "дага фесть азк",
					"transcription": "dagə fes't' azk"
				}
			]
		},
		{
			"id": 10,
			"translation": "Не понял",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "изине шарьходе",
					"transcription": "iz'in'ə šar̥'kəd'ə"
				}
			]
		},
		{
			"id": 11,
			"translation": "Не расслышал",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "изине куле",
					"transcription": "iz'in'ə kul'ə"
				}
			]
		},
		{
			"id": 12,
			"translation": "В разных районах неодинаковый язык",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "эрь районца кяльсь аф фкя",
					"transcription": "er' rajoncə kɛl's' af fkɛ"
				}
			]
		},
		{
			"id": 13,
			"translation": "Помедленнее говори",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сяда салаване корхтак",
					"transcription": "s'adə salavan'ə kor̥tak"
				}
			]
		},
		{
			"id": 14,
			"translation": "Наш мокшанский язык самый красивый",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мокшень кяленьке сембода мазы",
					"transcription": "mokšən' kɛl'ən'kə s'embədə mazi"
				}
			]
		},
		{
			"id": 15,
			"translation": "Он по-мокшански чисто говорит",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "сон мокшекс чистайста корхтай",
					"transcription": "son mokšəks čistajstə kor̥taj"
				}
			]
		},
		{
			"id": 16,
			"translation": "Наш мокшанский язык теряется",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мокшень кяленьке юмси",
					"transcription": "mokšən' kɛl'ən'kə jums'i"
				}
			]
		},
		{
			"id": 17,
			"translation": "Мокшанский язык надо сохранять",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "мокшень кяльть эряви ванфтомс",
					"transcription": "mokšən' kɛl't' er'avi vanftəms"
				}
			]
		},
		{
			"id": 18,
			"translation": "По-мокшански нужно говорить со своими детьми",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "эсь идентенть мархта эряви корхтамс мокшекс",
					"transcription": "es' id'ən't'ən' mar̥tə er'avi kor̥tams mokšəks"
				}
			]
		},
		{
			"id": 19,
			"translation": "Это эрзянское слово, наверное",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "тя арам эрзянь валсь",
					"transcription": "t'ɛ aram er'z'ɛn' vals' "
				}
			]
		},
		{
			"id": 20,
			"translation": "Говорите погромче",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "корхтада вишконяста",
					"transcription": "kor̥tadə viškən'astə"
				}
			]
		},
		{
			"id": 21,
			"translation": "Ты понимаешь, что я говорю?",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "тон шарьходсак, мезе мон корхтан?",
					"transcription": "ton šar̥'kəcak, mez'ə mon kor̥tan?"
				}
			]
		}
	]
}
