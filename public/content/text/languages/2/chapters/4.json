{
	"id": "4",
	"title": "4. Вопросы",
	"phrases": [
		{
			"translation": "Кто ты?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.1t.mp3"],
					"text": "Ken šie olet?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.1s.mp3"],
					"text": "Ken šie olet?"
				}
			]
		},
		{
			"translation": "Кто пойдет в музей?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.2t.mp3"],
					"text": "Ken lähtöy muzeih?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.2s.mp3"],
					"text": "Ken lähtöy musejoh?"
				}
			]
		},
		{
			"translation": "У кого есть ручка, карандаш?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.3t.mp3"],
					"text": "kellä on ručka, krandaša?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.3s.mp3"],
					"text": "Kellä on kynä?"
				}
			]
		},
		{
			"translation": "Кому отдать книгy?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.4t.mp3"],
					"text": "kellä andua kirjan?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.4s.mp3"],
					"text": "Kellä antua kirjan?"
				}
			]
		},
		{
			"translation": "С кем ты придешь?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.5t.mp3"],
					"text": "kenenke šie tulet?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.5s.mp3"],
					"text": "Kenen kera šie tulet?"
				}
			]
		},
		{
			"translation": "Что это?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.6t.mp3"],
					"text": "Mi tämä on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.6s.mp3"],
					"text": "Mi tämä on?"
				}
			]
		},
		{
			"translation": "Что ты сказал? ",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.7t.mp3"],
					"text": "Midä šie šanoit?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.7s.mp3"],
					"text": "Mitä šie šanoit?"
				}
			]
		},
		{
			"translation": "О чем ты расскажешь?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.8t.mp3"],
					"text": "Midä šie šanelet?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.8s.mp3"],
					"text": "Mistä šie kerrot?"
				}
			]
		},
		{
			"translation": "Чья это сумка?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.9t.mp3"],
					"text": "Kenen tämä sumča on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.9s.mp3"],
					"text": "Kenen tämä laukku on?"
				}
			]
		},
		{
			"translation": "Который автобус наш?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.10t.mp3"],
					"text": "Kumbane avtobussa on miän?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.10s.mp3"],
					"text": "Kumpani autobušši on meijän?"
				}
			]
		},
		{
			"translation": "Который (из двух)? из вас Антти?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.11t.mp3"],
					"text": "Ken teistä on Antti?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.11s.mp3"],
					"text": "Kumpi teistä on Antti?"
				}
			]
		},
		{
			"translation": "Которая (по счету) это остановка?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.12t.mp3"],
					"text": "Monešgo piettyš tämä on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.12s.mp3"],
					"text": "Moneško pyšäkki še on?"
				}
			]
		},
		{
			"translation": "Сколько стоит? ",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.13t.mp3"],
					"text": "äijägo tämä makšau?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.13s.mp3"],
					"text": "Äijänkö še makšau? Mitä še makšau?"
				}
			]
		},
		{
			"translation": "Сколько домов в деревне?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.14t.mp3"],
					"text": "äijägo taluo on kyläššä?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.14s.mp3"],
					"text": "Montako taluo kyläššä on?"
				}
			]
		},
		{
			"translation": "Когда поезд уходит?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.15t.mp3"],
					"text": "Konža pojezda lähtöy?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.15s.mp3"],
					"text": "Konša juna lähtöy?"
				}
			]
		},
		{
			"translation": "Как долго  ты будешь в Петрозаводске?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.16t.mp3"],
					"text": "min viikkuo šie lienet Petroskoissa?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.16s.mp3"],
					"text": "Kuin pitän aikua šie olet Petroskoissa?"
				}
			]
		},
		{
			"translation": "Где находится магазин?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.17t.mp3"],
					"text": "missä on laukka?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.17s.mp3"],
					"text": "Missä on kauppa?"
				}
			]
		},
		{
			"translation": "Kуда эта дорога ведет? ",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.18t.mp3"],
					"text": "kunne mänöy tämä tie?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.18s.mp3"],
					"text": "Kunne/minne tämä tie viey?"
				}
			]
		},
		{
			"translation": "Откуда дует?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.19t.mp3"],
					"text": "mistä tuulou?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.19s.mp3"],
					"text": "Mistä tuulou?"
				}
			]
		},
		{
			"translation": "Как  нам проехать на автовокзал?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.20t.mp3"],
					"text": "Kuin meilä ajua avtovokzualalla?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.20s.mp3"],
					"text": "Mitein myö piäšemmä autobuššiašemalla?"
				}
			]
		},
		{
			"translation": "Почему ты смеешься?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.21t.mp3"],
					"text": "Mintän šie nagrat?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.21s.mp3"],
					"text": "Mintäh šie nakrat?"
				}
			]
		},
		{
			"translation": "Во сколько ты встаешь?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.22t.mp3"],
					"text": "konža šie noužet?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.22s.mp3"],
					"text": "Moneltako šie noušet?"
				}
			]
		},
		{
			"translation": "На чем я доеду до вокзала?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.23t.mp3"],
					"text": "Millä miula ajua vokzualah?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.23s.mp3"],
					"text": "Millä mie piäšen ašemalla?"
				}
			]
		},
		{
			"translation": "Какое сегодня число?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.24t.mp3"],
					"text": "Monešgo päivä tänäpiänä on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.24s.mp3"],
					"text": "Moneško päivä tänäpiänä on?"
				}
			]
		},
		{
			"translation": "Что хочешь?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.25t.mp3"],
					"text": "midä tahot?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.25s.mp3"],
					"text": "Mitä tahot?"
				}
			]
		},
		{
			"translation": "Который час?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.26t.mp3"],
					"text": "äijägo aigua on?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.26s.mp3"],
					"text": "Äijänkö kello on?"
				}
			]
		},
		{
			"translation": "Смогу ли я дойти до театра пешком?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.27t.mp3"],
					"text": "voičengo mie kopittua teatrah šuat jalgaziin?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.27s.mp3"],
					"text": "Piäšenkö mie teatterih aštumalla?"
				}
			]
		},
		{
			"translation": "Сколько времени  мне надо ждать?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/4/4.28t.mp3"],
					"text": "äijägo aigua miula pidäy vuottua?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/4/4.28s.mp3"],
					"text": "Pitän aikuako/pitältikö miun pitäy vuottua?"
				}
			]
		}
	]
}
