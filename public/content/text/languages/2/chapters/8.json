			"translation": "мама",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "тядяй",
					"transcription": "t'ɛd'ɛj"
				}
			]
		},
		{
			"id": 60,
			"translation": "папа",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "аляй",
					"transcription": "al'aj"
				}
			]
		},
		{
			"id": 61,
			"translation": "бабушка (по матери)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "шавай",
					"transcription": "šavaj"
				}
			]
		},
		{
			"id": 62,
			"translation": "дедушка (по матери)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "щятяй",
					"transcription": "šɛt'ɛj"
				}
			]
		},
		{
			"id": 63,
			"translation": "бабушка (по отцу)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "атяй",
					"transcription": "at'aj"
				}
			]
		},
		{
			"id": 64,
			"translation": "дедушка (по отцу)",
			"dialects": [
				{
					"id": "1",
					"title": "standard",
					"text": "бабай",
					"transcription": "babaj"
				}
			]
		},
		{
			"id": 65,
			"translation": "пра-(бабушка/дедушка)",
			"dialects": [
				{
					"id": "1",
