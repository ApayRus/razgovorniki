{
	"id": "8",
	"title": "8. Досуг",
	"phrases": [
		{
			"translation": "Что делаешь вечером?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.1t.mp3"],
					"text": "midä ruat illalla?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.1s.mp3"],
					"text": "Mitä ruat illalla? "
				}
			]
		},
		{
			"translation": "Когда ты свободен?",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.2t.mp3"],
					"text": "konža šie olet joudava?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.2s.mp3"],
					"text": "Konša šiula on aikua?"
				}
			]
		},
		{
			"translation": "Пойдем на концерт.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.3t.mp3"],
					"text": "läkki koncertah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.3s.mp3"],
					"text": "Läkkä konserttih"
				}
			]
		},
		{
			"translation": "Поехали на праздник.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.4t.mp3"],
					"text": "läkki pruazniekalla"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.4s.mp3"],
					"text": "Läkkä pruasniekkah yheššä"
				}
			]
		},
		{
			"translation": "Пойдем в гости.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.5t.mp3"],
					"text": "läkki gostih"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.5s.mp3"],
					"text": "Läkkä gostih"
				}
			]
		},
		{
			"translation": "Пойдем гулять.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.6t.mp3"],
					"text": "läkki gul’aimah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.6s.mp3"],
					"text": "Tuletko kävelömäh"
				}
			]
		},
		{
			"translation": "А не хочешь в театр/на выставку.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.7t.mp3"],
					"text": "etgo taho lähtie teatrah/ kačondapaikkah?"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.7s.mp3"],
					"text": "Etkö taho lähtie teatterih/näyttelyh?"
				}
			]
		},
		{
			"translation": "Встретимся в семь.",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.8t.mp3"],
					"text": "vaštuačemma šeiččimen čuasun aigah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.8s.mp3"],
					"text": "Vaštauvumma kello šeiččemen"
				}
			]
		},
		{
			"translation": "Встретимся утром/днем/вечером",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.9t.mp3"],
					"text": "vaštuačemma huomnekšella / päivällä / illalla"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.9s.mp3"],
					"text": "Vaštauvumma huomenekšella/päivällä/illalla"
				}
			]
		},
		{
			"translation": "Приходи на чай",
			"dialects": [
				{
					"title": "тверской",
					"audios": ["/content/audio/1/1/8/8.10t.mp3"],
					"text": "tule čuajuo juomah"
				},
				{
					"title": "северный",
					"audios": ["/content/audio/1/2/8/8.10s.mp3"],
					"text": "Tule čäijyö juomah"
				}
			]
		}
	]
}
